<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/radius_top_bg">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/large_fan_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_large_fan_20" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="@+id/large_fan_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/semicircle" />

        <ImageView
            android:id="@+id/spray_large_head_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6"
            app:layout_constraintEnd_toStartOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/spray_large_head_2"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="4dp"
            android:rotation="90"
            app:layout_constraintBottom_toTopOf="@+id/imageView6"
            app:layout_constraintEnd_toEndOf="@+id/imageView6"
            app:layout_constraintStart_toStartOf="@+id/imageView6"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:rotation="180"
            app:layout_constraintBottom_toBottomOf="@+id/spray_large_head_1"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:layout_constraintTop_toBottomOf="@+id/spray_large_head_2"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp"
            android:rotation="180"
            app:layout_constraintBottom_toBottomOf="@+id/spray_large_head_2"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            app:layout_constraintTop_toTopOf="@+id/spray_large_head_2"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            app:layout_constraintBottom_toBottomOf="@+id/large_fan_1"
            app:layout_constraintEnd_toEndOf="@+id/spray_large_head_1"
            app:layout_constraintStart_toStartOf="@+id/spray_large_head_1"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotation="30"
            app:layout_constraintBottom_toTopOf="@+id/spray_large_head_2"
            app:layout_constraintEnd_toEndOf="@+id/spray_large_head_5"
            app:layout_constraintHorizontal_bias="0.134"
            app:layout_constraintStart_toStartOf="@+id/spray_large_head_2"
            app:layout_constraintTop_toBottomOf="@+id/spray_large_head_5"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotation="60"
            app:layout_constraintBottom_toTopOf="@+id/spray_large_head_1"
            app:layout_constraintEnd_toEndOf="@+id/spray_large_head_5"
            app:layout_constraintStart_toStartOf="@+id/spray_large_head_2"
            app:layout_constraintTop_toTopOf="@+id/spray_large_head_5"
            app:layout_constraintVertical_bias="0.134"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotation="120"
            app:layout_constraintBottom_toBottomOf="@+id/spray_large_head_8"
            app:layout_constraintEnd_toEndOf="@+id/spray_large_head_8"
            app:layout_constraintStart_toStartOf="@+id/spray_large_head_5"
            app:layout_constraintTop_toTopOf="@+id/spray_large_head_5"
            app:layout_constraintVertical_bias="0.134"
            app:srcCompat="@drawable/progress_20_round" />

        <ImageView
            android:id="@+id/spray_large_head_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:rotation="150"
            app:layout_constraintBottom_toTopOf="@+id/spray_large_head_5"
            app:layout_constraintEnd_toEndOf="@+id/spray_large_head_8"
            app:layout_constraintHorizontal_bias="0.866"
            app:layout_constraintStart_toStartOf="@+id/spray_large_head_5"
            app:layout_constraintTop_toBottomOf="@+id/spray_large_head_8"
            app:srcCompat="@drawable/progress_20_round" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>